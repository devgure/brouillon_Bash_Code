backend/app/config.py
import os
from pydantic import BaseSettings

class Settings(BaseSettings):
    MONGO_URI: str = os.getenv("MONGO_URI", "mongodb://mongo:27017/mediagent")
    REDIS_URL: str = os.getenv("REDIS_URL", "redis://redis:6379/0")
    SECRET_KEY: str = os.getenv("SECRET_KEY", "supersecret")
    STRIPE_SECRET: str = os.getenv("STRIPE_SECRET", "")
    STRIPE_WEBHOOK_SECRET: str = os.getenv("STRIPE_WEBHOOK_SECRET", "")
    MODEL_PROVIDER: str = os.getenv("MODEL_PROVIDER", "openai")
    OPENAI_API_KEY: str = os.getenv("OPENAI_API_KEY", "")
    CLAUDE_API_KEY: str = os.getenv("CLAUDE_API_KEY", "")
    VECTOR_DB_URL: str = os.getenv("VECTOR_DB_URL", "http://vectordb:8000")
    SENTRY_DSN: str = os.getenv("SENTRY_DSN", "")

settings = Settings()

backend/app/main.py
from fastapi import FastAPI
from fastapi.middleware.cors import CORSMiddleware
from motor.motor_asyncio import AsyncIOMotorClient
from app.config import settings

app = FastAPI(title="MediAgent API")

app.add_middleware(
    CORSMiddleware,
    allow_origins=["*"],  # restrict in prod
    allow_credentials=True,
    allow_methods=["*"],
    allow_headers=["*"],
)

# DB client
mongo_client = AsyncIOMotorClient(settings.MONGO_URI)
db = mongo_client.get_default_database()

# Include routers
from app.api.v1 import auth, intake, eligibility, priors, billing, billing_webhook
app.include_router(auth.router, prefix="/api/v1/auth", tags=["auth"])
app.include_router(intake.router, prefix="/api/v1/intake", tags=["intake"])
app.include_router(eligibility.router, prefix="/api/v1/eligibility", tags=["eligibility"])
app.include_router(priors.router, prefix="/api/v1/priors", tags=["priors"])
app.include_router(billing.router, prefix="/api/v1/billing", tags=["billing"])
app.include_router(billing_webhook.router, prefix="/api/v1/webhooks/stripe", tags=["stripe"])

@app.get("/health")
async def health():
    return {"status": "ok"}

backend/app/models.py (Mongo models via Pydantic for FastAPI)
from pydantic import BaseModel, Field
from typing import Optional, Any, List
from datetime import datetime

class User(BaseModel):
    id: Optional[str] = Field(None, alias="_id")
    email: str
    name: str
    company: Optional[str]
    role: str = "user"
    created_at: datetime = datetime.utcnow()

class PatientIntake(BaseModel):
    id: Optional[str] = Field(None, alias="_id")
    patient_name: str
    dob: Optional[str]
    contact: Optional[str]
    raw_form: dict
    status: str = "pending"
    created_at: datetime = datetime.utcnow()

class AgentJob(BaseModel):
    id: Optional[str] = Field(None, alias="_id")
    job_type: str
    input_data: dict
    status: str = "queued"
    result: Optional[dict] = None
    logs: List[dict] = []
    created_at: datetime = datetime.utcnow()

backend/app/api/v1/intake.py (example route)
from fastapi import APIRouter, Depends, HTTPException
from app.main import db
from app.models import PatientIntake
from app.workers.tasks import enqueue_agent_job
from bson import ObjectId

router = APIRouter()

@router.post("/submit")
async def submit_intake(payload: dict):
    # Save raw intake
    doc = {
        "patient_name": payload.get("name"),
        "dob": payload.get("dob"),
        "contact": payload.get("contact"),
        "raw_form": payload,
        "status": "queued"
    }
    res = await db.intakes.insert_one(doc)
    job = {
        "job_type": "intake_extraction",
        "input_data": {"intake_id": str(res.inserted_id)}
    }
    await enqueue_agent_job(job)
    return {"status": "queued", "id": str(res.inserted_id)}

backend/app/workers/tasks.py (enqueue + minimal worker)
import json
from celery import Celery
from app.config import settings
from motor.motor_asyncio import AsyncIOMotorClient
from bson import ObjectId
import asyncio

celery = Celery(__name__, broker=settings.REDIS_URL, backend=settings.REDIS_URL)

mongo = AsyncIOMotorClient(settings.MONGO_URI).get_default_database()

@celery.task(name="enqueue_agent_job")
def enqueue_agent_job(job):
    # In production: push into Celery queue with routing to specific worker
    # For demo, we will enqueue a celery task that runs the agent flow
    run_agent_job.delay(job)

@celery.task(name="run_agent_job")
def run_agent_job(job):
    # This runs synchronously in the worker; use model provider to call LLMs.
    from app.services.agent_orchestrator import orchestrate_job
    orchestrate_job(job)

backend/app/services/agent_orchestrator.py (simplified)
from app.config import settings
from app.services.model_provider import ModelProvider
from motor.motor_asyncio import AsyncIOMotorClient

mongo = AsyncIOMotorClient(settings.MONGO_URI).get_default_database()
provider = ModelProvider()

def orchestrate_job(job: dict):
    job_type = job.get("job_type")
    if job_type == "intake_extraction":
        intake_id = job["input_data"]["intake_id"]
        # fetch intake doc
        intake = mongo.intakes.find_one({"_id": ObjectId(intake_id)})
        # TODO: run extraction -> call model provider
        extracted = provider.extract_intake(intake["raw_form"])
        # save results and create followup tasks (eligibility, prior auth, billing)
        mongo.intakes.update_one({"_id": ObjectId(intake_id)}, {"$set": {"status": "processed", "extracted": extracted}})
        # chain next job
        next_job = {"job_type": "eligibility_check", "input_data": {"intake_id": intake_id}}
        enqueue_next(next_job)
    # add other job types...

backend/app/services/model_provider.py (abstraction)
import os
from app.config import settings
from typing import Dict

class ModelProvider:
    def __init__(self):
        self.provider = settings.MODEL_PROVIDER

    def call(self, prompt: str, **kwargs) -> Dict:
        if self.provider == "openai":
            return self._call_openai(prompt, **kwargs)
        elif self.provider == "claude":
            return self._call_claude(prompt, **kwargs)
        # fallback or other providers

    def _call_openai(self, prompt: str, **kwargs):
        import openai
        openai.api_key = settings.OPENAI_API_KEY
        resp = openai.ChatCompletion.create(
            model="gpt-4o-mini",
            messages=[{"role": "user", "content": prompt}],
            **kwargs
        )
        return resp

    def extract_intake(self, raw_form: dict) -> dict:
        prompt = f"Extract patient intake fields from: {raw_form}"
        resp = self.call(prompt, max_tokens=512)
        # parse resp
        return {"parsed": resp}

backend/app/api/v1/billing_webhook.py (Stripe webhook)
from fastapi import APIRouter, Request, HTTPException
from app.config import settings
import stripe

router = APIRouter()
stripe.api_key = settings.STRIPE_SECRET

@router.post("/stripe")
async def stripe_webhook(request: Request):
    payload = await request.body()
    sig_header = request.headers.get("stripe-signature")
    try:
        event = stripe.Webhook.construct_event(payload, sig_header, settings.STRIPE_WEBHOOK_SECRET)
    except ValueError:
        raise HTTPException(status_code=400, detail="Invalid payload")
    except stripe.error.SignatureVerificationError:
        raise HTTPException(status_code=400, detail="Invalid signature")
    # handle event types
    if event["type"] == "checkout.session.completed":
        session = event["data"]["object"]
        # mark user subscription
    return {"status": "ok"}

